Create DATABASE Obligatorio2;
USE Obligatorio2
/*
DELETE FROM Donacion;
DELETE FROM Visualizacion;
DELETE FROM Contenido;
DELETE FROM Categoria;
DELETE FROM Usuario;
DROP Table Donacion;
DROP Table Visualizacion;
DROP Table Contenido;
DROP Table Categoria;
DROP Table Usuario;
*/
-----------------------------------------------------------
-- 1. Script de creación de las tablas
-----------------------------------------------------------
CREATE TABLE Usuario(
Email VARCHAR(30) NOT NULL,
Nombre VARCHAR(30) NOT NULL,
Usuario VARCHAR(10) NOT NULL,
Contraseña VARCHAR(8) NOT NULL,
Sexo VARCHAR(1) NOT NULL,
FechaNac DATE NOT NULL,
FechaAlta DATE NOT NULL
);

CREATE TABLE Categoria (
CodCategoria INTEGER IDENTITY(1,1),
NombreCategoria VARCHAR(8) NOT NULL,
);

CREATE TABLE Contenido(
CodContenido INT NOT NULL,
Titulo VARCHAR(40) NOT NULL,
FechaEmision DATE NOT NULL,
EmailUsuario VARCHAR(30) NOT NULL,
CodCategoria INT NOT NULL,
Estado VARCHAR(8) NOT NULL,
);

CREATE TABLE Visualizacion(
CodContenido INT NOT NULL,
EmailUsuario VARCHAR(30) NOT NULL,
Fecha DATE NOT NULL,
);

CREATE TABLE Donacion(
EmailOrigen VARCHAR(30) NOT NULL,
EmailDestino VARCHAR(30) NOT NULL,
FechaCreacion DATE NOT NULL,
FechaAcreditacion DATE,
Monto DECIMAL(9,2) NOT NULL,
EstadoDonacion VARCHAR(9) NOT NULL
);

-----------------------------------------------------------
-- 1. Script ADD CONSTRAINT de las tablas
-----------------------------------------------------------

ALTER TABLE Usuario
	ADD CONSTRAINT Usuario_pk PRIMARY KEY(Email);
ALTER TABLE Usuario
	ADD CONSTRAINT uc_Usuario_Usuario UNIQUE (Usuario);
ALTER TABLE Usuario
	ADD CONSTRAINT ck_Usuario_Sexo CHECK ((UPPER(Sexo)) in ('F', 'M', 'O'));
ALTER TABLE Usuario
	ADD CONSTRAINT ck_Usuario_FechaNac CHECK (DATEDIFF(YEAR, FechaNac, GETDATE())>15);

ALTER TABLE Categoria
	ADD CONSTRAINT Categoria_pk PRIMARY KEY(CodCategoria);
ALTER TABLE Categoria
	ADD CONSTRAINT uc_Categoria_NombreCategoria UNIQUE (NombreCategoria);
ALTER TABLE Categoria
	ADD CONSTRAINT ck_Categoria_NombreCategoria CHECK ((UPPER(NombreCategoria)) in ('JUEGOS', 'IRL', 'MUSICA' , 'ESPORTS' , 'CREATIVE'));

ALTER TABLE Contenido
	ADD CONSTRAINT Contenido_pk PRIMARY KEY(CodContenido);
ALTER TABLE Contenido
        ADD CONSTRAINT fk_Contenido_Usuario_Email FOREIGN KEY (EmailUsuario) REFERENCES Usuario(Email);
ALTER TABLE Contenido
        ADD CONSTRAINT fk_Contenido_Categoria_CodCategoría FOREIGN KEY (CodCategoria) REFERENCES Categoria(CodCategoria);
ALTER TABLE Contenido
	ADD CONSTRAINT ck_Contenido_Estado CHECK ((UPPER(Estado)) in ('PUBLICO', 'PRIVADO'));
ALTER TABLE Contenido
	ADD CONSTRAINT ck_longitud_CodContenido  CHECK (CodContenido >= 1000 AND CodContenido <= 9999)

ALTER TABLE Visualizacion
	ADD CONSTRAINT Visualizacion_pk PRIMARY KEY(EmailUsuario,CodContenido,Fecha);
ALTER TABLE Visualizacion
        ADD CONSTRAINT fk_Visualizacion_Usuario_Email FOREIGN KEY (EmailUsuario) REFERENCES Usuario(Email);
ALTER TABLE Visualizacion
        ADD CONSTRAINT fk_Visualizacion_Categoria_CodContenido FOREIGN KEY (CodContenido) REFERENCES Contenido(CodContenido);

ALTER TABLE Donacion
	ADD CONSTRAINT Donacion_pk PRIMARY KEY(EmailOrigen, EmailDestino, FechaCreacion) ;
ALTER TABLE Donacion
        ADD CONSTRAINT fk_Usuario_EmailOrigen FOREIGN KEY (EmailOrigen) REFERENCES Usuario(Email);
ALTER TABLE Donacion
        ADD CONSTRAINT fk_Usuario_EmailDestino FOREIGN KEY (EmailDestino) REFERENCES Usuario(Email);
ALTER TABLE Donacion
	ADD CONSTRAINT ck_Contenido_EstadoDonacion CHECK ((UPPER(EstadoDonacion)) in ('PENDIENTE', 'APROBADA'));
ALTER TABLE Donacion
	ADD CONSTRAINT ck_FechaAcreditacion CHECK ((FechaAcreditacion IS NULL) OR ((UPPER(EstadoDonacion)) <> 'PENDIENTE'));
ALTER TABLE Donacion
	ADD CONSTRAINT ck_Contenido_EstadoDonacio_Diferente CHECK (EmailOrigen != EmailDestino);
ALTER TABLE Donacion
	ADD CONSTRAINT c_Donacion_Monto CHECK (Monto > 0);

